#include <iostream>

using namespace std;

int main()
{
    int a,b,r;
    cout<<"Ingresar valor: ";
    cin>>a;
    cout<<"Ingresar valor: ";
    cin>>b;
    r=a+b;
    cout<<"La suma es: "<<r<<endl;
    r=a*b;
    cout<<"La multiplicacion es: "<<r<<endl;
    r=a/b;
    cout<<"La division es: "<<r<<endl;
    return 0;
}
